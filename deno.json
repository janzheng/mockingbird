{
  "tasks": {
    "serve": "deno serve --port 9990 --allow-sys --allow-read --allow-import --allow-env --allow-write --allow-net --reload=https://esm.town ./main.js",
    "search": "deno run --allow-env --allow-net --allow-read ./core/search/groq-compound/test.js",
    "search:tavily": "deno run --allow-env --allow-net --allow-read ./core/search/tavily/test.js",
    "search:jina": "deno run --allow-env --allow-net --allow-read ./core/search/jina/test.js",
    "search:exa": "deno run --allow-env --allow-net --allow-read ./core/search/exa-search/test.js",
    "search:exa-r": "deno run --allow-env --allow-net --allow-read ./core/search/exa-research/test.js",
    "scrape:tavily": "deno run --allow-env --allow-net --allow-read ./core/scrape/tavily/test.js",
    "scrape:jina": "deno run --allow-env --allow-net --allow-read ./core/scrape/jina/test.js"
  },
  "$schema": "https://raw.githubusercontent.com/denoland/deno/98f62cee78e85bfc47c62ed703777c6bc8794f1c/cli/schemas/config-file.v1.json",
  "lock": false,
  "compilerOptions": {
    "noImplicitAny": false,
    "strict": true,
    "types": ["https://www.val.town/types/valtown.d.ts"],
    "lib": [
      "dom",
      "dom.iterable",
      "dom.asynciterable",
      "deno.ns",
      "deno.unstable"
    ]
  },
  "lint": {
    "include": ["deno:/https/esm.town/**/*"],
    "rules": {
      "exclude": ["no-explicit-any"]
    }
  },
  "node_modules_dir": false,
  "experimental": {
    "unstable-node-globals": true,
    "unstable-temporal": true,
    "unstable-worker-options": true,
    "unstable-sloppy-imports": true
  }
}